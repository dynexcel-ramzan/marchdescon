<?xml version="1.0" ?>
<odoo>
    <data>
          <record id="project_project_form_view_project_timesheet" model="ir.ui.view">
			<field name="name">project.project.form.view.timesheet</field>
			<field name="model">project.project</field>
			<field name="inherit_id" ref="project.edit_project"/>
			<field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                     <page string="ORA Configuration">
                         <group>
                             <group>
                                 <field name="ora_client_id" />
                                 <field name="ora_category_id" />
                                 <field name="ora_enabled" />
                                 <field name="ora_job_duration" />
                                 <field name="ora_status" />
                                 <field name="ora_record_id" invisible="1"/>
                             </group>
                             <group>
                                  <field name="ora_mobilization_date" />
                                  <field name="ora_close_date" />
                                  <field name="ora_code" />
                                  <field name="ora_city" />
                                  <field name="ora_region" />

                             </group>
                         </group>
                     </page>
				</xpath>
            </field>
		</record>

    </data>
</odoo>